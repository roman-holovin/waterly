    "deploy": "gh-pages -d build",
    "report:coverage": "codeclimate-test-reporter < coverage/lcov.info",
    "storybook": "start-storybook -s public",
    "build-storybook": "build-storybook -s public",
    "precommit": "lint-staged"
  }
}
